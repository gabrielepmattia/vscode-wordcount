{
	"name": "wordcount",
	"displayName": "Word Count",
	"version": "0.0.1",
	"publisher": "gabrielepmattia",
	"description": "Word count with reading time",
	"author": {
		"name": "Gabriele Proietti Mattia",
		"email": "pm.gabriele@outlook.com"
	},
	"categories": [
		"Other"
	],
	"license": "SEE LICENSE IN LICENSE.txt",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"activationEvents": [
		"onLanguage:markdown"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/gabrielepmattia/vscode-wordcount"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
